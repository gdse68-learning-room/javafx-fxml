<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootNode" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.javafx.controller.DashboardFormController">
   <children>
      <Button layoutX="25.0" layoutY="26.0" mnemonicParsing="false" onAction="#btnCustomerOnAction" prefHeight="47.0" prefWidth="118.0" style="-fx-background-color: #ff7979;" text="Customer">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="257.0" layoutY="26.0" mnemonicParsing="false" onAction="#btnItemOnAction" prefHeight="47.0" prefWidth="118.0" style="-fx-background-color: #e056fd;" text="Item">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="460.0" layoutY="26.0" mnemonicParsing="false" onAction="#btnSupplierOnAction" prefHeight="47.0" prefWidth="118.0" style="-fx-background-color: #30336b;" text="Supplier" textFill="WHITE">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
